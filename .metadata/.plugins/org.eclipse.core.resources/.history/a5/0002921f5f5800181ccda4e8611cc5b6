package hr.fer.zemris.java.gui.layouts;

/**
 * @author Blaz Bagic
 * @version 1.0
 */
public class RCPosition {
  private int row;
  private int column;

  public RCPosition(int row, int column) {
    super();
    this.row = row;
    this.column = column;
  }

  public int getRow() {
    return row;
  }

  public int getColumn() {
    return column;
  }
}
